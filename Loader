if getgenv().BinbinvoGamingHub == "PC" then
    loadstring(game:HttpGet("https://binbinvogamingvnclone.github.io/PcLoader",true))()
elseif getgenv().BinbinvoGamingHub == "Mobile" then
    loadstring(game:HttpGet("https://binbinvogamingvnclone.github.io/MobleLoader",true))()
elseif getgenv().BinbinvoGamingHubVersion == "V2" then
    if getgenv().BinbinvoGamingHub == "PC" then
        loadstring(game:HttpGet("https://BinbinvoGamingVNClone.github.io/AllV2PCLoader.lua",true))()
    elseif getgenv().BinbinvoGamingHub == "Mobile" then
        loadstring(game:HttpGet("https://BinbinvoGamingVNClone.github.io/AllV2MobleLoader.lua",true))()
elseif getgenv().BinbinvoGamingHubVersion == "V1" then
        if getgenv().BinbinvoGamingHub == "PC" then
            loadstring(game:HttpGet("https://BinbinvoGamingVNClone.github.io/AllV1PCLoader.lua",true))()
        elseif getgenv().BinbinvoGamingHub == "Mobile" then
            loadstring(game:HttpGet("https://BinbinvoGamingVNClone.github.io/AllV1MobleLoader.lua",true))()
